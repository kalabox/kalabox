{
  "name": "kalabox",
  "description": "A framework to build reusable, super fast, highly customizable, extensible integrated workflow solutions for all kinds of apps.",
  "license": "MIT",
  "keywords": [
    "docker",
    "kalabox",
    "drupal",
    "wordpress",
    "devops"
  ],
  "main": "lib/kbox.js",
  "version": "2.1.0-rc.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/kalabox/kalabox"
  },
  "bugs": {
    "email": "mike@kalabox.io",
    "url": "https://github.com/kalabox/kalabox/issues"
  },
  "bin": {
    "kbox": "./bin/kbox.js"
  },
  "directories": {
    "lib": "./lib",
    "bin": "./bin",
    "src": "./src"
  },
  "window": {
    "title": "Kalabox",
    "toolbar": true,
    "frame": true,
    "width": 700,
    "height": 520,
    "max_width": 700,
    "max_height": 520,
    "min_width": 700,
    "min_height": 520,
    "resizeable": false,
    "icon": "./src/images/kalaboxv2-ico.png"
  },
  "nwjsBuilder": {
    "productName": "Kalabox",
    "copyright": "2016",
    "comments": "Make local dev great again!",
    "companyName": "Kalabox Inc."
  },
  "chromium-args": "--enable-threaded-compositing",
  "web_accessible_resources": [
    "*.png"
  ],
  "dependencies": {
    "bluebird": "^3.4.1",
    "chalk": "~0.5.1",
    "decompress": "^3.0.0",
    "dockerode": "^2.2.7",
    "download": "^4.4.3",
    "fs-extra": "^0.18.2",
    "inquirer": "^0.8.3",
    "js-yaml": "^3.4.6",
    "jsonfile": "^2.4.0",
