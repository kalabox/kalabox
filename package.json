    "lodash": "^3.10.1",
    "lpad-align": "^1.0.0",
    "meow": "^3.3.0",
    "micromatch": "^2.3.7",
    "minimatch": "2 || 3",
    "mkdirp": "^0.5.1",
    "moment": "^2.10.6",
    "multipipe": "^0.1.2",
    "redis": "https://github.com/thinktandem/node_redis/tarball/master",
    "restler": "^3.4.0",
    "sha1": "^1.1.0",
    "shell-escape": "https://github.com/thinktandem/node-shell-escape/tarball/master",
    "shelljs": "^0.6.0",
    "sync-exec": "^0.6.2",
    "uuid": "^2.0.1",
    "verror": "^1.8.1",
    "winston": "^0.9.0",
    "yargs": "^3.7.0"
  },
  "devDependencies": {
    "bats": "git+https://github.com/sstephenson/bats.git#master",
    "ben": "0.0.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.3.0",
    "grunt": "^0.4.5",
    "grunt-bower-install-simple": "^1.0.0",
    "grunt-bump": "0.7.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compress": "^1.2.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-sass": "^0.9.2",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-html-angular-validate": "^0.5.7",
    "grunt-html2js": "^0.3.5",
    "grunt-jscs": "~0.8.1",
    "grunt-jsdoc": "^0.5.7",
    "grunt-mdlint": "0.0.1",
    "grunt-mocha-cli": "^2.0.0",
    "grunt-nwjs-builder": "thinktandem/grunt-nwjs-builder.git",
    "grunt-shell": "^1.1.1",
    "jshint-stylish": "~1.0.0",
    "jxcore": "file:./modules/jxcore",
    "matchdep": "^0.3.0",
    "mock-fs": "^3.5.0",
    "nwjs-builder": "^1.12.0",
